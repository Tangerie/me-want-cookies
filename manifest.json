{
	"author": "Tangerie",
	"name": "me-want-cookies",
	"version": "0.0.1",
	"description": "Export all Cookies of the current Tab",
	"icons": {
		"48": "design/icon-48.png",
		"128": "design/icon-128.png"
	},
	"background": {
    	"service_worker": "chrome/service.mjs",
		"type": "module"
  	},
	"permissions": [
		"cookies",
		"tabs",
		"scripting",
		"activeTab"
	],
	"host_permissions": [
		"https://thecavillgroup.agentboxcrm.com.au/*",
		"https://rpp.corelogic.com.au/*"
	],
	"manifest_version": 3,
	"offline_enabled": true
}
